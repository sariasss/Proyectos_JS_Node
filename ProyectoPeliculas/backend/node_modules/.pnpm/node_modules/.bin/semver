#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/semver@7.7.1/node_modules/semver/bin/node_modules:/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/semver@7.7.1/node_modules/semver/node_modules:/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/semver@7.7.1/node_modules:/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/semver@7.7.1/node_modules/semver/bin/node_modules:/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/semver@7.7.1/node_modules/semver/node_modules:/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/semver@7.7.1/node_modules:/mnt/c/Users/saraa/Documents/React/Curso-React-2025/proyectos/04-routes7-cine-context/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
